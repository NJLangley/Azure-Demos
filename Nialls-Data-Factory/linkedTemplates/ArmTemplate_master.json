{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Nialls-Data-Factory"
		},
		"Nialls_Data_Lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://niallsdatalake.dfs.core.windows.net"
		},
		"Nialls_Keys_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Nialls-Keys.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Nialls_Keys_properties_typeProperties_baseUrl": {
						"value": "[parameters('Nialls_Keys_properties_typeProperties_baseUrl')]"
					},
					"Nialls_Data_Lake_properties_typeProperties_url": {
						"value": "[parameters('Nialls_Data_Lake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}